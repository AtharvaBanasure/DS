1. ServerInterface

	import java.util.*;
	import java.rmi.*;
	import java.rmi.server.*;
	
	interface ServerInterface extends Remote {
		public void Additon(double a,double b)throws RemoteException;
	}


2. ServerImplemrnt
	import java.util.*;
	import java.rmi.*;
	import java.rmi.server.*;
	
	ServerImplement extends UnicastRemoteObject implements ServerInterface{
		try
			public ServerImplement()throws RemoteException
			public void Additon(double a,double b)throws RemoteException;	
		catch
	}

3. Server

	public class Server{
		ServerImplemrnt si=new ServerImplemrnt()
		Naming.rebind("Server",si);
		SOP(Server Started);
	}


4. Client

	public class Client{
		String url="rmi://localhost/Server";
		ServerImplemrnt si=Naming.lookup(url);
		
		si.Additon();
		
	}
